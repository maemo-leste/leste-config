Section "InputClass"
        Identifier "calibration"
        MatchProduct "Atmel maXTouch Touchscreen"
        Option "Ignore"         "true"
        Option "Calibration"   "-5 1026 -5 964"
EndSection

Section "InputClass"
        Identifier "calibration"
        MatchProduct "Filtered Touchscreen"
        Option "Calibration"   "-5 1026 -5 964"
EndSection
